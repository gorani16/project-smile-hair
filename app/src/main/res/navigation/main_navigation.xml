<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.gorani.smilehair.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_home_to_example"
            app:destination="@id/navigation_example" />
    </fragment>
    <fragment
        android:id="@+id/navigation_reservation"
        android:name="com.gorani.smilehair.ReservationFragment"
        android:label="ReservationFragment" />
    <fragment
        android:id="@+id/navigation_board"
        android:name="com.gorani.smilehair.BoardFragment"
        android:label="BoardFragment" />
    <fragment
        android:id="@+id/navigation_mypage"
        android:name="com.gorani.smilehair.MyPageFragment"
        android:label="MyPageFragment" />
    <fragment
        android:id="@+id/navigation_example"
        android:name="com.gorani.smilehair.ExampleFragment"
        android:label="ExampleFragment" />
</navigation>